{
	/* CodeIgniter Query Builder Class */
	"CodeIgniter Database INSERT": {
		"prefix": "db_insert",
		"body": [
			"\\$this->db->insert('${1:table}', $${2:data});$0"
		],
		"description": "CodeIgniter Database INSERT"
	},
	"CodeIgniter Database REPLACE": {
		"prefix": "db_replace",
		"body": [
			"\\$this->db->replace('${1:table}', $${2:data});$0"
		],
		"description": "CodeIgniter Database REPLACE"
	},
	"CodeIgniter Database UPDATE": {
		"prefix": "db_update",
		"body": [
			"\\$this->db->where('${1:field}', $${2:value});",
			"\\$this->db->update('${3:table}', $${4:data});$0"
		],
		"description": "CodeIgniter Database UPDATE"
	},
	"CodeIgniter Database DELETE": {
