	"CodeIgniter Set Cookie": {
		"prefix": "set_cookie",
		"body": [
			"$${1:cookie} = array(",
			"\t'name'   => '${2:The Cookie Name}',",
			"\t'value'  => '${3:The Value}',",
			"\t'expire' => '${4:86500}',",
			"\t'domain' => '${5:.some-domain.com}',",
			"\t'path'   => '${6:/}',",
			"\t'prefix' => '${7:myprefix_}',",
			"\t'secure' => ${8:TRUE}",
			");",
			"\\$this->input->set_cookie($${1})$0"
		],
		"description": "CodeIgniter Set Cookie"
	},
